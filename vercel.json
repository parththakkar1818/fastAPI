{
    "version": 2,
    "builds": [
        {
            "src": "/app/api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/getprice",
            "methods": ["GET"],
            "dest": "/app/api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/app/api.py"
        }
    ],
    "functions": {
        "api/getprice.py": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}
